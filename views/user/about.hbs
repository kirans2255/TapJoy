    <script>
        function addToWishlist(productId) {
            fetch(`/wishlist/${productId}`, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ userId, productId })
            })
                .then(response => {
                    if (!response.ok) {
                        console.log("a")
                        throw new Error('Failed to add product to wishlist');
                    }

                    return response.json();
                })
                .then(response => {
                    console.log("pop", response)
                    if (response.errorMessage === 'Removed from Wishlist successfully') {
                        Toastify({
                            text: "Removed From Wishlist",
                            duration: 1000, // 3 seconds
                            gravity: "top", // Display at the top
                            position: 'right', // Display on the right side
                            backgroundColor: "black", // Background color
                            stopOnFocus: true, // Stop the timer when the toast is focused
                        }).showToast();
                    }
                    else {
                        Toastify({
                            text: "Added to Wishlist",
                            duration: 1000, // 3 seconds
                            gravity: "top", // Display at the top
                            position: 'right', // Display on the right side
                            backgroundColor: "black", // Background color
                            stopOnFocus: true, // Stop the timer when the toast is focused
                        }).showToast();
                    }
                    console.log("b")

                    // Handle success message or update UI as needed
                    console.log(response.message);
                })
                .catch(error => {
                    console.error('Error:', error);
                    // Handle error or update UI as needed
                });
        }

    </script>

